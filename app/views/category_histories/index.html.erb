<h1>Listing category_histories</h1>

<table>
  <tr>
    <th>Category</th>
    <th>Member</th>
    <th>Complete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @category_histories.each do |category_history| %>
  <tr>
    <td><%= category_history.category_id %></td>
    <td><%= category_history.member_id %></td>
    <td><%= category_history.complete %></td>
    <td><%= link_to 'Show', category_history %></td>
    <td><%= link_to 'Edit', edit_category_history_path(category_history) %></td>
    <td><%= link_to 'Destroy', category_history, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

